{
  "version": "2.0.0-alpha.9",
  "npmClient": "yarn",
  "command": {
    "exec": {
      "ignore": ["wxparcel-create-app"]
    },
    "publish": {
      "npmClient": "npm",
      "ignoreChanges": ["ignored-file", "*.md"],
      "message": "chore(release): publish",
      "registry": "https://registry.npmjs.org/"
    }
  },
  "packages": ["@wxparcel/*", "@wxparcel-transformer/*", "@wxparcel-loader/*", "@wxparcel-plugin/*", "@wxparcel-config/*", "@wxparcel-create/*"]
}
