    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2",
    "typescript": "^3.7.5",
    "wxparcel-core": "^2.0.0-alpha.6"
  }
